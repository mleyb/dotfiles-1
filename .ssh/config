Host *
    IdentityFile ~/.ssh/id_rsa
    ControlMaster auto
    ControlPath ~/.ssh/socket-%r@%h:%p
    Compression yes
    AddressFamily inet
    ServerAliveInterval 120
    ForwardX11 no
    ForwardAgent  no

Host linode
    HostName mvdan.cc
    User mvdan

Host dev1
    HostName dev1.confine-project.eu
    User dani

Host 192.168.*.*
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null
Host 10.139.40.*
    User root
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host sc1
	HostName 10.139.94.115
	User root

Host sc2
	HostName 10.139.40.50
	User root
